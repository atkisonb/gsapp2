<%- model_class = TradeBook -%>
<div class="page-header">
  <h1><%=t '.title', :default => TradeBook %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:stock_id) %></dt>
      <dd><pre class="prettyprint"><%= @trade_book.stock_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:trade_time) %></dt>
      <dd><pre class="prettyprint"><%= @trade_book.trade_time %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:price) %></dt>
      <dd><pre class="prettyprint"><%= @trade_book.price %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              trade_books_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_trade_book_path(@trade_book), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              trade_book_path(@trade_book),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

