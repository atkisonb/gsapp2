<h1>Listing fund_stocks</h1>

<table>
  <thead>
    <tr>
      <th>Fund</th>
      <th>Stock</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fund_stocks.each do |fund_stock| %>
      <tr>
        <td><%= fund_stock.fund_id %></td>
        <td><%= fund_stock.stock_id %></td>
        <td><%= link_to 'Show', fund_stock %></td>
        <td><%= link_to 'Edit', edit_fund_stock_path(fund_stock) %></td>
        <td><%= link_to 'Destroy', fund_stock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fund stock', new_fund_stock_path %>
